    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "html-react-parser": "^4.2.1",
    "isomorphic-dompurify": "^1.8.0",
    "next": "14.0.1",
    "npm": "^10.0.0",
    "postcss": "8.4.28",
